<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
"http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
 <listener>
       <listener-class>org.ms123.launcher.OsgiStarter</listener-class>
    </listener>
  <servlet>
    <servlet-name>simpl4</servlet-name>
    <servlet-class>org.ms123.launcher.LauncherProxyServlet</servlet-class>
    <init-param>
      <param-name>maxThreads</param-name>
      <param-value>20</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>simpl4</servlet-name>
    <url-pattern>/simpl4/*</url-pattern>
  </servlet-mapping>
</web-app>
